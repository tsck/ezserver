{
  "name": "ezserver",
  "description": "Simple static server",
  "version": "3.1.0",
  "main": "src/index.js",
  "author": "Terrence Keane",
  "license": "MIT",
  "homepage": "https://github.com/tsck/ezserver#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsck/ezserver.git"
  },
  "bugs": {
    "url": "https://github.com/tsck/ezserver/issues"
  },
  "keywords": [
    "simple",
    "http",
    "server"
  ],
  "devDependencies": {
    "commitizen": "2.8.6",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.12.2",
    "ghooks": "1.3.2"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint --ignore-pattern *.test.js src"
  },
  "files": [
    "src",
    "README.md"
  ],
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "colors": "1.1.2",
    "mime-types": "2.1.13"
  }
}
